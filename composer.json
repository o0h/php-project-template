{
  "name": "vendor/package",
  "description": "This is the awesome your application.",
  "type": "library",
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-phpunit": "^0.12.16",
    "phpstan/phpstan-strict-rules": "^0.12.5",
    "phpunit/phpunit": "^9.0",
    "psalm/plugin-phpunit": "^0.12.1",
    "slevomat/coding-standard": "^6.4",
    "squizlabs/php_codesniffer": "^3.0",
    "vimeo/psalm": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Vendor\\Package\\": "src/",
      "Vendor\\Package\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "lint" : [
      "@cs",
      "@stan",
      "@psalm"
    ],
    "cs": "phpcs",
    "stan": "phpstan analyse",
    "psalm": "psalm"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Hideki Kinjyo",
      "email": "dev@o0h.in"
    }
  ],
  "config": {
    "sort-packages": true
  }
}
